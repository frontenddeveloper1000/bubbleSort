const bubbleSort =(arr) => {
 //outer loop.
 for ( i=arr.length; i>=0; i--){
    //inner loop.
    for(j=0;j<=arr.length;j++){
      if(arr[j-1]>arr[j]){
        //swap the indices.
        temp =arr[j-1];
        arr[j-1]=arr[j];
      arr[j]=temp; }
      }
    
     }
  return arr;
     }

console.log(bubbleSort([7,8,4,3,2]));
